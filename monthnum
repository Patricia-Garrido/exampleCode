var monthMap = {'1':'January',
		'2' : 'February',
		'3' : 'March',
		'4' : 'April',
		'5' : 'May',
		'6' : 'June',
		'7' : 'July',
		'8' : 'August',
		'9' : 'September',
		'10' : 'October',
		'11' : 'November',
		'12' : 'December',
};
function getMonthName(month){
	 if (month == undefined || month > 12 || month < 1){
      	return 'Enter a number between 1 & 12';
  	}
	return monthMap [month];
}
var n = '22';
var x = getMonthName (n);
console.log (x);